{
    "presets": [
        ["@babel/preset-env", { "modules": false }],
        ["@babel/preset-react"]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime", ["import",
            { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }
        ],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        [
            "styled-jsx/babel",
            { "plugins": ["styled-jsx-plugin-less"] }
        ]
    ]
}